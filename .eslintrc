{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-shadow": 0,
        "quotes": [2, "single"],
        "curly": [2, "multi"]
    }
}

